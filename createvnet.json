      "dependsOn": [ ],
      "tags": {
        "displayName": "ABC"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('ABCPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('ABCSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('ABCSubnet1Prefix')]"
            }
          }
          }
        ]
      }
    }],
  "outputs": {}
}
